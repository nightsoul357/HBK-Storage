@inherits CredentialComponentBase<WebDAVStorageCredentials>

<MudItem xs="12" sm="8" md="6">
    <MudTextField @bind-Value="base._credentials.Url" Label="Url" Variant="Variant.Outlined" Required="true" />
</MudItem>
<MudItem xs="12" sm="8" md="6">
    <MudTextField @bind-Value="base._credentials.Username" Label="Username" Variant="Variant.Outlined" Required="true" />
</MudItem>
<MudItem xs="12" sm="8" md="6">
    <MudTextField @bind-Value="base._credentials.Password"  InputType="InputType.Password" Label="Password" Variant="Variant.Outlined" Required="true" />
</MudItem>
<MudItem xs="12" sm="8" md="6">
    <MudCheckBox @bind-Checked="base._credentials.IsSupportPartialUpload" Color="Color.Primary" Required="true">Force Path Style</MudCheckBox>
</MudItem>